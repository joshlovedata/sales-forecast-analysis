    "# Load the datasets\n",
    "features_df = pd.read_csv('features.csv')\n",
    "stores_df = pd.read_csv('stores.csv')\n",
    "train_df = pd.read_csv('train.csv')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "8555f790-1870-4d69-be3a-68c362762524",
   "metadata": {},
   "outputs": [],
   "source": [
    "#  Data Cleaning and Preparation\n",
    "# Convert the Date column in features_df to datetime format\n",
    "features_df['Date'] = pd.to_datetime(features_df['Date'])\n",
    "train_df['Date'] = pd.to_datetime(train_df['Date'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "b3187dff-2e00-43d6-b3bb-e5dce1b42c3a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Merge train_df with features_df and stores_df to create a comprehensive dataset\n",
    "merged_df = train_df.merge(features_df, on=['Store', 'Date'], how='left')\n",
    "merged_df = merged_df.merge(stores_df, on='Store', how='left')\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "c7b33208-e587-4563-8833-bd7c4d434ca1",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 2: Aggregate weekly sales across all stores and departments by date\n",
    "aggregated_sales = merged_df.groupby('Date')['Weekly_Sales'].sum().reset_index()\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "495a9543-adc3-45e9-890c-f9e6037e4cab",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Step 3: Feature engineering: Adding time-based features for analysis\n",
    "aggregated_sales['Year'] = aggregated_sales['Date'].dt.year\n",
    "aggregated_sales['Month'] = aggregated_sales['Date'].dt.month\n",
    "aggregated_sales['Week'] = aggregated_sales['Date'].dt.isocalendar().week"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "id": "b9139a9b-3640-46c2-afb7-4659a9930b4f",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate a 4-week rolling average for forecasting\n",
    "aggregated_sales['Rolling_Avg'] = aggregated_sales['Weekly_Sales'].rolling(window=4).mean()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "f614709c-d518-46ce-8eaf-41599fcb6251",
   "metadata": {},
   "outputs": [

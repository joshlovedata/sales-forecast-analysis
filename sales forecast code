   ],
   "source": [
    "# Step 4: Plotting the original sales and the rolling average forecast\n",
    "plt.figure(figsize=(12, 6))\n",
    "plt.plot(aggregated_sales['Date'], aggregated_sales['Weekly_Sales'], label='Actual Weekly Sales')\n",
    "plt.plot(aggregated_sales['Date'], aggregated_sales['Rolling_Avg'], label='4-Week Rolling Forecast', linestyle='--', color='orange')\n",
    "plt.title('Sales Forecast Using Rolling Average')\n",
    "plt.xlabel('Date')\n",
    "plt.ylabel('Weekly Sales')\n",
    "plt.legend()\n",
    "plt.show()"
   ]
